//
// threedim.spl -- indexing in a three-dimensional array
//


type V = array [5] of int;
type M = array [5] of V;
type A = array [5] of M;


proc main() {
  var a: A;
  var i: int;
  var j: int;
  var k: int;

  i := 0;
  while (i < 5) {
    j := 0;
    while (j < 5) {
      k := 0;
      while (k < 5) {
        if (i = j) {
          if (j = k) {
            a[i][j][k] := 1;
          } else {
            a[i][j][k] := 0;
          }
        } else {
          a[i][j][k] := 0;
	}
	k := k + 1;
      }
      j := j + 1;
    }
    i := i + 1;
  }
  i := 0;
  while (i < 5) {
    j := 0;
    while (j < 5) {
      k := 0;
      while(k < 5) {
        printi(a[i][j][k]);
	k := k + 1;
      }
      j := j + 1;
      printc('\n');
    }
    printc('\n');
    i := i + 1;
  }
}
