type A = array [10] of int;


proc sort(ref a: A) {
  var i: int;
  var j: int;
  var t: int;

  i := 9;
  while (i >= 0) {
    j := 1;
    while (j <= i) {
      if (a[j - 1] > a[j]) {
        t := a[j - 1];
        a[j - 1] := a[j];
        a[j] := t;
      }
      j := j + 1;
    }
    i := i - 1;
  }
}


