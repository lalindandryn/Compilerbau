//
// partly indexed array
//

type A = array[7] of int;
type B = array[5] of A;

proc p(ref x: A) {
  var i: int;

  i := 0;
  while (i < 7) {
    x[i] := i;
    i := i + 1;
  }
}

proc main() {
  var b: B;
  var i: int;
  var j: int;

  i := 0;
  while (i < 5) {
    j := 0;
    while (j < 7) {
      b[i][j] := 0;
      j := j + 1;
    }
    i := i + 1;
  }
  p(b[2]);
  i := 0;
  while (i < 5) {
    j := 0;
    while (j < 7) {
      printi(b[i][j]);
      printc(' ');
      j := j + 1;
    }
    printc('\n');
    i := i + 1;
  }
}
